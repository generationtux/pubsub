{
    "name": "generationtux/pubsub",
    "description": "A Lumen & Laravel package for Google Pub/Sub or Amazon SNS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Troy Harvey",
            "email": "troy.harvey@generationtux.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "illuminate/config": "^5.1",
        "illuminate/http": "^5.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "GenTux\\PubSub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GenTux\\PubSub\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
