{
    "name": "generationtux/google-pubsub",
    "description": "Google Pub/Sub push queue Iron.io alternative for Lumen, Laravel, and PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Troy Harvey",
            "email": "troy.harvey@generationtux.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "0.9.*",
        "illuminate/http": "^5.1",
        "illuminate/config": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "GenTux\\GooglePubSub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GenTux\\GooglePubSub\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
